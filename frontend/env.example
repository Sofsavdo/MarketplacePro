# AFFILIMART Frontend Environment Variables

# API Configuration
API_URL=http://localhost:5000
NEXT_PUBLIC_API_URL=http://localhost:5000

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-key

# Payment Configuration
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key

# OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret

# Analytics
NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token
NEXT_PUBLIC_AMPLITUDE_API_KEY=your-amplitude-api-key
NEXT_PUBLIC_GA_TRACKING_ID=your-google-analytics-id

# File Storage
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Feature Flags
NEXT_PUBLIC_ENABLE_SMART_REWARDS=true
NEXT_PUBLIC_ENABLE_FRAUD_DETECTION=true
NEXT_PUBLIC_ENABLE_GAMIFICATION=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_LOYALTY_PROGRAM=true

# Internationalization
NEXT_PUBLIC_DEFAULT_LOCALE=uz
NEXT_PUBLIC_SUPPORTED_LOCALES=uz,ru,en,ar

# SEO
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME=AFFILIMART
NEXT_PUBLIC_SITE_DESCRIPTION=Marketplace va Affiliate Network

# WebSocket
NEXT_PUBLIC_WS_URL=ws://localhost:5001

# Development
NODE_ENV=development 